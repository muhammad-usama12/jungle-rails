  <header class="page-header">
    <h1 align="center">Login</h1>
  </header>

<div align="center"><%= form_tag '/login' do %>
<form>
  <table>
    <tr>
      <td align="right">Email <%= text_field_tag :email %></td>
    </tr>
    <tr>
      <td align="right">Password <%= password_field_tag :password %></td>
    </tr>
  </table><br>
    <i align="center"><%= submit_tag "Submit",  class: "btn btn-outline-success" %></i>
</form>
<% end %>